<?xml version="1.0" encoding="UTF-8"?>

<odoo>

	<template id="_assets_utils" name="Mixins" inherit_id="web._assets_utils" priority="15">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/mixins.scss"/>
		</xpath>
	</template>

	<template id="_assets_primary_variables" name="Colors" inherit_id="web._assets_primary_variables" priority="15">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/colors.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/variables.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/reset_variables_odoo.scss"/>
		</xpath>
	</template>

	<template id="assets_common" name="Common Assets" inherit_id="web_editor.assets_common">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/fonts.scss"/>
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/bemo_theme/static/src/js/core/search.js" />
		</xpath>
	</template>

	<template id="assets_backend" name="Backend Assets" inherit_id="web.assets_backend">
		<xpath expr="//link[last()]" position="after">
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/ui.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/libs/simplebar/simplebar.scss" />
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/layout.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/navbar.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/apps.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/appsbar.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/control_panel.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/search_panel.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/search_view.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/list_view.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/messaging_menu.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/dropdown.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/web.zoomodoo.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/kanban_view.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/activity_view.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/form_view.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/fields.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/fields_extra.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/notification.scss"/>
			<link rel="stylesheet" type="text/scss" href="/bemo_theme/static/src/scss/settings.scss"/>
		</xpath>
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/web/static/lib/jquery.touchSwipe/jquery.touchSwipe.js" />
			<script type="text/javascript" src="/bemo_theme/static/libs/simplebar/simplebar.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/libs/scrollbar.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/chrome/actions.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/chrome/menu.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/chrome/apps.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/chrome/appsbar.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/fields/relational.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/views/form_view.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/views/form_renderer.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/views/kanban_column.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/views/kanban_renderer.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/js/views/kanban_quick_create.js" />
			<script type="text/javascript" src="/bemo_theme/static/src/components/control_panel.js" />
		</xpath>
	</template>

</odoo>
