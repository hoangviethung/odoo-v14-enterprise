            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/colors.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/variables.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/reset_variables_odoo.scss"/>
        </xpath>
    </template>

    <template id="bemo_request_kanban_css_assets_backend" inherit_id="web.assets_backend" name="Bemo Request Kanban Css Assests Backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/request_kanban.scss"/>
        </xpath>
    </template>

    <template id="bemo_assets_common" inherit_id="web_editor.assets_common" name="Bemo Assets Common">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/fonts.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/wysiwyg.scss"/>
        </xpath>

        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/bemo_theme_backend/static/src/js/core/search.js" />
        </xpath>
    </template>

