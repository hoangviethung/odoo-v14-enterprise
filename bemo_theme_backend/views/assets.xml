<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <template id="bemo_assets_utils" inherit_id="web._assets_utils" name="Bemo Assests Utils" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/bemo_theme_backend/static/src/scss/mixins.scss"/>
        </xpath>
    </template>

    <template id="bemo_assets_primary_variables" inherit_id="web._assets_primary_variables" name="Bemo Assest Primary Variables " priority="15">
