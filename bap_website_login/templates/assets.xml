<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="bemo_assets_frontend" inherit_id="web.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/reset.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/navbar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/bemo-website.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/utilities.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/forms.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/header.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/footer.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/scss/login.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/components/bemo-hero-banner.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/components/bemo-notification.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/components/bemo-company-intention.scss"/>
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/components/bemo-home-news.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/bap_website_login/static/src/components/_main.js"/>
        </xpath>
    </template>

    <template id="beme_libs_css_frontend" inherit_id="web._assets_helpers">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/bap_website_login/static/src/libs/swiper-bundle.scss"/>
        </xpath>

    </template>

    <template id="bemo_libs_js_frontend" inherit_id="web._assets_common_minimal_js">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/bap_website_login/static/src/libs/swiper-bundle.js"></script>
        </xpath>
    </template>
</odoo>